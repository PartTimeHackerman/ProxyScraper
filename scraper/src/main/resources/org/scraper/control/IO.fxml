<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scraper.control.IOController">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="save" mnemonicParsing="false" text="Save" />
            <Button fx:id="load" mnemonicParsing="false" text="Load" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</VBox>
