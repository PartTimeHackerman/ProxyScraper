<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="198.0" prefWidth="116.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scraper.control.IOController">
   <children>
      <TilePane alignment="CENTER" prefHeight="35.0" prefWidth="135.0">
         <children>
            <TextArea fx:id="addArea" minHeight="-Infinity" prefHeight="26.0" prefWidth="47.0" promptText=" ADD" />
         </children>
      </TilePane>
      <Button fx:id="save" mnemonicParsing="false" text="Save" />
      <Button fx:id="load" mnemonicParsing="false" text="Load" />
   </children>
</VBox>
