<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scraper.control.ExecuteController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="465.0" spacing="20.0">
          <children>
              <Button fx:id="scrapeButton" mnemonicParsing="false" text="Scrape" />
              <Button fx:id="checkButton" mnemonicParsing="false" text="Check" />
              <VBox alignment="CENTER" prefHeight="98.0" prefWidth="92.0">
                  <children>
                      <Button fx:id="gatherButton" mnemonicParsing="false" text="Gather" textAlignment="CENTER">
                          <opaqueInsets>
                              <Insets />
                          </opaqueInsets>
                          <VBox.margin>
                              <Insets top="5.0" />
                          </VBox.margin>
                      </Button>
                      <HBox alignment="TOP_CENTER" prefHeight="24.0" prefWidth="100.0">
                          <children>
                              <Label text="Depth" />
                              <TextField fx:id="depthField" alignment="CENTER" prefHeight="8.0" prefWidth="27.0" />
                          </children>
                          <opaqueInsets>
                              <Insets />
                          </opaqueInsets>
                          <VBox.margin>
                              <Insets top="5.0" />
                          </VBox.margin>
                      </HBox>
                  </children>
                  <HBox.margin>
                      <Insets />
                  </HBox.margin>
              </VBox>
              <CheckBox fx:id="checkOnFly" mnemonicParsing="false" text="Check on fly" />
            <Label text="Threads" />
            <TextField fx:id="depthField1" alignment="CENTER" prefHeight="8.0" prefWidth="27.0" />
          </children>
      </HBox>
      <Label fx:id="uiLog" prefHeight="18.0" prefWidth="465.0" />
   </children>
</VBox>
